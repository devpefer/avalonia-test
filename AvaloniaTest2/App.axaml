<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AvaloniaTest2.App"
             xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
             xmlns:viewModels="clr-namespace:AvaloniaTest2.ViewModels"
             xmlns:views="clr-namespace:AvaloniaTest2.Views"
             xmlns:global="clr-namespace:"
             RequestedThemeVariant="Default">

    <Application.DataTemplates>
        <DataTemplate DataType="viewModels:FileExplorerViewModel">
            <views:FileExplorer/>
        </DataTemplate>

        <DataTemplate DataType="viewModels:ServersViewModel">
            <views:Servers/>
        </DataTemplate>
    </Application.DataTemplates>

    <Application.Styles>
        <SimpleTheme />
    </Application.Styles>
</Application>